
@model BlogApp.Models.DisplayEntryViewModel
<h1>This is a blog</h1>
@Html.ActionLink("Add Entry", "AddEntry","Entry")
@{
    foreach(var entries in Model.Entries)
    {
        <fieldset>
        <div>@entries[1]  @entries[2]</div> 
        <div>@entries[3]</div>
            @Html.ActionLink("edit", "EditEntry", "Entry", new { Id = entries[0] }, null)
        </fieldset>
    }
}


